{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "eventelektro": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/eventelektro",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/theme/shop-master-theme/sass/bootstrap.scss",
              "src/assets/theme/shop-master-theme/sass/style.scss",
              "src/assets/theme/shop-master-theme/css/bootstrap.css",
              "src/assets/theme/shop-master-theme/css/flexslider.css",
              "src/assets/theme/shop-master-theme/css/icomoon.css",
              "src/assets/theme/shop-master-theme/css/owl.carousel.min.css",
              "src/assets/theme/shop-master-theme/css/owl.theme.default.min.css",
              "src/assets/theme/shop-master-theme/css/style.css",
              "src/styles.scss",
              "node_modules/animate.css/animate.min.css"
            ],
            "scripts": [
              "src/assets/theme/shop-master-theme/js/jquery.min.js",
              "src/assets/theme/shop-master-theme/js/jquery.countTo.js",
              "src/assets/theme/shop-master-theme/js/jquery.easing.1.3.js",
              "src/assets/theme/shop-master-theme/js/jquery.flexslider-min.js",
              "src/assets/theme/shop-master-theme/js/jquery.waypoints.min.js",
              "src/assets/theme/shop-master-theme/js/bootstrap.min.js",
              "src/assets/theme/shop-master-theme/js/main.js",
              "src/assets/theme/shop-master-theme/js/modernizr-2.6.2.min.js",
              "src/assets/theme/shop-master-theme/js/owl.carousel.min.js",
              "src/assets/theme/shop-master-theme/js/respond.min.js",
              "src/assets/theme/shop-master-theme/js/google_map.js",
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "eventelektro:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "eventelektro:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "eventelektro:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "eventelektro-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "eventelektro:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "eventelektro:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "eventelektro"
}